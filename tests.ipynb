    "def print_slow(str):\n",
    "    from time import sleep\n",
    "    from os import sys \n",
    "    for letter in str:\n",
    "        sys.stdout.write(letter)\n",
    "        sys.stdout.flush()\n",
    "        sleep(0.07)\n",
    "\n",
    "print_slow(\"João arrasou na pesquisa\")"
